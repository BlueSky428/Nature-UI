{
  "name": "@nature-ui/alert",
  "version": "2.2.12",
  "description": "A React component used to alert users of a particular screen area that needs user action",
  "keywords": [
    "alert",
    "notice",
    "notification",
    "react",
    "nature ui",
    "ui"
  ],
  "sideEffects": false,
  "author": "Divine Hycenth <divinehycenth8@gmail.com>",
  "license": "MIT",
  "main": "src/index.ts",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DNature/nature-ui.git"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package"
  },
  "dependencies": {
    "@nature-ui/icon": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@nature-ui/layout": "workspace:*",
    "@nature-ui/system": "workspace:*",
    "@nature-ui/utils": "workspace:*",
    "clean-package": "^2.1.1",
    "react": "^18.0.0"
  },
  "bugs": {
    "url": "https://github.com/DNature/nature-ui/issues"
  },
  "homepage": "https://github.com/DNature/nature-ui#readme",
  "gitHead": "cafae1d72b2a7027028454c22d4bbaebfdafba5f",
  "clean-package": "../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
