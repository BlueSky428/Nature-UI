{
  "name": "@nature-ui/form-control",
  "version": "0.0.0-dev-20230409203113",
  "description": "React component to provide validation states to form fields",
  "keywords": [
    "field",
    "input",
    "input group",
    "form",
    "theming",
    "validation",
    "error",
    "react",
    "nature ui"
  ],
  "author": "Divine Hycenth <divinehycenth8@gmail.com>",
  "homepage": "https://github.com/DNature/nature-ui",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DNature/nature-ui.git"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "bugs": {
    "url": "https://github.com/DNature/nature-ui/issues"
  },
  "dependencies": {
    "@nature-ui/hooks": "workspace:*",
    "@nature-ui/icon": "workspace:*",
    "@nature-ui/react-utils": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@nature-ui/system": "workspace:*",
    "@nature-ui/utils": "workspace:*",
    "clean-package": "^2.1.1",
    "react": "^18.0.0"
  },
  "gitHead": "cafae1d72b2a7027028454c22d4bbaebfdafba5f",
  "clean-package": "../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
