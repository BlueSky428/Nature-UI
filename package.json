{
  "name": "nature-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "tooling/*"
    ],
    "nohoist": [
      "packages/apps/**/webpack-dev-server",
      "@emotion/core/types",
      "**/@emotion/core/types"
    ]
  },
  "author": "Divine Hycenth <divinehycenth8@gmail.com>",
  "license": "MIT",
  "scripts": {
    "update:check": "ncu",
    "update": "ncu -u",
    "prepare": "husky install .config/husky",
    "pkg": "manypkg run",
    "check:pkgs": "manypkg check",
    "fix:pkgs": "manypkg fix",
    "version:pkgs": "changeset version",
    "contributors:add": "all-contributors add",
    "contributors:gen": "all-contributors generate",
    "members:gen": "ts-node scripts/all-members.ts",
    "search-meta:gen": "ts-node scripts/get-search-meta.ts",
    "prestart": "yarn && yarn bootstrap",
    "boot": "yarn prestart && yarn build",
    "start": "yarn build && yarn storybook",
    "lint:eslint": "eslint packages/**/src/ --ext .ts,.tsx --config .eslintrc",
    "lint:types": "tsc --noEmit",
    "lint": "concurrently yarn:lint:*",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --maxWorkers=25% --watch --onlyChanged",
    "test:ci": "jest --color --runInBand --ci",
    "build:babel": "yarn pkg babel-plugin build",
    "build": "yarn build:babel && preconstruct build",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "build:tailwind": "postcss ./.storybook/style.css -o ./.storybook/tailwind.css",
    "storybook": "env NODE_ENV=development start-storybook -p 5001",
    "commit": "cz",
    "prettier": "prettier --write \"**/*.{ts,tsx,json,md,yml}\"",
    "prerelease": "yarn build",
    "v-release": "lerna run v-pub",
    "build:storybook": "build-storybook && sb extract",
    "release": "changeset publish",
    "docs": "yarn workspace @nature-ui/docs",
    "docs:build": "yarn docs build",
    "docs:dev": "yarn docs dev",
    "docs:start": "yarn docs start"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.19.4",
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@manypkg/cli": "^0.19.2",
    "@preconstruct/cli": "^2.2.2",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@swc-node/jest": "^1.5.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/autoprefixer": "^10.2.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@types/postcss-import": "^14.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/tailwindcss": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "all-contributors-cli": "^6.24.0",
    "autoprefixer": "^10.4.12",
    "babel-eslint": "^10.1.0",
    "babel-preset-react-app": "^10.0.1",
    "chart.js": "^3.9.1",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "commitizen": "^4.2.5",
    "concurrently": "^7.4.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.13",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.7.2",
    "execa": "^6.1.0",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "jest-watch-typeahead": "^2.2.0",
    "lerna": "^6.0.1",
    "lint-staged": "^13.0.3",
    "node-sass": "^7.0.3",
    "npm-check-updates": "^16.3.12",
    "postcss": "^8.4.18",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-chartjs-2": "^4.3.1",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.37.0",
    "react-icons": "^4.6.0",
    "react-lorem-component": "^0.13.0",
    "react-router-dom": "^6.4.2",
    "rimraf": "^3.0.2",
    "storybook": "^6.5.12",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
